        "laravel-echo": "^1.5.3",
        "lodash": "^4.17.20",
        "moment": "^2.24.0",
        "pluralise": "^1.0.0",
        "pusher-js": "^4.4.0",
        "quill-image-drop-module": "^1.0.3",
        "vee-validate": "^2.1.7",
        "vue": "^2.6.6",
